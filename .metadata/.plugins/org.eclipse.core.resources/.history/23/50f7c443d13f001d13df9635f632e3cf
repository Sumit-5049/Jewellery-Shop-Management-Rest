package com.virtusa.jsm.dto;

import java.sql.Timestamp;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.OneToOne;
import javax.persistence.Table;

@Entity
@Table(name = "bill_table")
public class Bill {

	@Id
	int id;
	Timestamp dateTime;
	@OneToOne
	Order order;
	Double tax;
	Double total;
}
