	public Bill(int billid, Timestamp dateTime, int o, int cust_id, int shop_id) {
		super();
		this.billid = billid;
		this.dateTime = dateTime;
		this.ord_id = o;
		this.cust_id = cust_id;
		this.shop_id = shop_id;
	}



